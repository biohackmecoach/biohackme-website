# Firebase Functions Environment Variables
# Copy this file to .env and fill in your actual values
# Keep .env secure and never commit it to git

# OpenAI API Configuration (Server-side only)
OPENAI_API_KEY=your_openai_api_key_here

# Stripe Configuration (Server-side only)
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Mailchimp Configuration (Server-side only)
# Get your API key from: https://us4.admin.mailchimp.com/account/api/
MAILCHIMP_API_KEY=your_mailchimp_api_key_here
MAILCHIMP_AUDIENCE_ID=your_mailchimp_audience_id_here
MAILCHIMP_DATA_CENTER=us4
